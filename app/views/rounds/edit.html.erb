<h1>Editing Round</h1>

<% form_for(@round) do |f| %>
  <%= f.error_messages %>

<fieldset>
    <legend>Round</legend>
    <%= render :partial => "form", :locals=>{ :f => f} %>
</fieldset>

  <fieldset class="button_holder">
    <%= f.submit "Update" %>
  </fieldset>
<% end %>

<div class="bottom_nav">
<%= link_to 'Show', @round %> |
<%= link_to 'Back', rounds_path %>
</div>