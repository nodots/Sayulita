<h1>Listing Institution_people</h1>

<table class="listing_table">
  <tr>
    <th>Institution</th>
    <th>Person</th>
    <th>Institution person type</th>
    <th colspan="3">Actions</th>
  </tr>

<% for institution_person in @institution_people %>
  <tr>
    <td><%=h institution_person.institution_id %></td>
    <td><%=h institution_person.person_id %></td>
    <td><%=h institution_person.institution_person_type %></td>
    <td><%= link_to 'Show', institution_person %></td>
    <td><%= link_to 'Edit', edit_institution_person_path(institution_person) %></td>
    <td><%= link_to 'Delete', institution_person, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<div class="bottom_nav">
<%= link_to 'New institution_person', new_institution_person_path %>
</div>