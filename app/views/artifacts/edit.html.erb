<h1>Editing Artifact</h1>

<% form_for(@artifact) do |f| %>
  <%= f.error_messages %>

<fieldset>
    <legend>Artifact</legend>
    <%= render :partial => "form", :locals=>{ :f => f} %>
</fieldset>

  <fieldset class="button_holder">
    <%= f.submit "Update" %>
  </fieldset>
<% end %>

<div class="bottom_nav">
<%= link_to 'Show', @artifact %> |
<%= link_to 'Back', artifacts_path %>
</div>